        "libs/generic-table/data-access/src/index.ts"
      ],
      "@local/generic-table/feature/shell": [
        "libs/generic-table/feature/shell/src/index.ts"
      ],
      "@local/shared/data-access": ["libs/shared/data-access/src/index.ts"],
      "@local/shared/feature/generic-table": [
        "libs/shared/feature/generic-table/src/index.ts"
      ],
      "@local/shared/feature/table": ["libs/shared/feature/table/src/index.ts"],
      "@local/shared/ui": ["libs/shared/ui/src/index.ts"],
      "@local/shared/ui/ui": ["libs/shared/ui/ui/src/index.ts"]
