{
  "name": "angular-material-feature-table",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/angular-material/feature/table/src",
  "prefix": "local-angular-material",
  "tags": ["scope:angular-material", "type:feature"],
  "projectType": "library",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/angular-material/feature/table/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/angular-material/feature/table/**/*.ts",
          "libs/angular-material/feature/table/**/*.html"
        ]
      }
    }
  }
}
