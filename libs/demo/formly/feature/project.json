{
  "name": "demo-formly",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "feature/src",
  "prefix": "local-demo",
  "tags": ["scope:demo", "type:feature"],
  "projectType": "library",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/demo/formly/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/demo/formly/**/*.ts",
          "libs/demo/formly/**/*.html"
        ]
      }
    }
  }
}
